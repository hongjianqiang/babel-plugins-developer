{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "debug": false,
                "useBuiltIns": "usage",
                "targets": [">= 0.2%", "ios_saf >= 7", "not ie <= 8"],
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": false,
                "useESModules": true
            }
        ],
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ]
    ]
}
